check process enketo matching app.js
   start program = "/bin/systemctl start pm2-enketo.service"
   stop program = "/bin/systemctl stop pm2-enketo.service"
    if failed port 8005 type tcp for 3 cycles then restart
    if 20 restarts within 20 cycles then exec /etc/monit/slack_notifications.sh
    if 20 restarts within 20 cycles then timeout
    group pm2
